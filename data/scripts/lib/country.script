begin
   Result := 0;
   if (upgradeid<>'') then
   begin
      var i : Integer;
      for i:=0 to gc_country_maxupgradecount-1 do
      begin
         if ((gCountry[cid].upgrade[i].id=upgradeid) or ((bAddIfNotExist) and (gCountry[cid].upgrade[i].id=''))) then
         begin
            Result := i;
            break;
         end;
      end;
   end
   else
   Log('_country_GetUpgradeIndexByUpgradeID : upgradeid="');
end;

